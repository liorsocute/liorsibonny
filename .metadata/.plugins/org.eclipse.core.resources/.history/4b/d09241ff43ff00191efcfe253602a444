package homework;

public class First_time {

	public static void main(String[] args) {

		int x = (int) (Math.random() * 10);
		int y = (int) (Math.random() * 10);

		if (x != y) {
			if (x < y) {
				System.out.printf("%d is begger than %d", y, x);
			} else {
				System.out.printf("%d id begger than %d", x, y);
			}
		} else {
			System.out.printf("they even %d %d", x, y);
		}
	}
}